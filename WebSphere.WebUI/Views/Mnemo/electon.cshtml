<script type="text/javascript">

    var modelId = '@Model.Id';
	var modelname='@Model.Name';
	var modelfullname='@Model.FullName';
    function openLink(link) {
        window.open(link.replace('idModel', modelId), '_self');
    } 
    function openUst(link) {
        link = link.replace('parentName', '@Model.ParentName').replace('Port', '@Model.Port').replace('realName', '@Model.RealName').replace(' ', '%20');
        loadLink(link);
    }
    function upd() {
        LoadPageOff();
	InitMnemoSVG('@Model.Name', '@Model.FullName');}
    window.addEventListener("load", function() {InitMnemoSVG('@Model.Name', '@Model.FullName');});


</script>
 